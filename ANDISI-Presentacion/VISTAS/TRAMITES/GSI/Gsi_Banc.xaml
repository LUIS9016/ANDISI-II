<UserControl x:Class="ANDISI_Presentacion.VISTAS.TRAMITES.GSI.Gsi_Banc"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:andi="clr-namespace:ANDISI_Presentacion.RECURSOS.CONTROLES"
             mc:Ignorable="d">
    <Border  Style="{StaticResource Border-tab}">
        <Grid>
            <Grid.RowDefinitions>
                <RowDefinition Height="*"/>
                <RowDefinition Height="*"/>
            </Grid.RowDefinitions>
            <Grid Grid.Row="0">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="*"></ColumnDefinition>
                    <ColumnDefinition Width="*"></ColumnDefinition>
                    <ColumnDefinition Width="*"></ColumnDefinition>
                </Grid.ColumnDefinitions>
                <Grid Grid.Column="0">
                    <StackPanel Style="{StaticResource stack_margin-controls}" >
                        <Label   Content="No. Socio" />
                        <TextBox Style="{StaticResource control_heigh}" />
                    </StackPanel>
                </Grid>
                <Grid Grid.Column="1">
                    <StackPanel Style="{StaticResource stack_margin-controls}">
                        <Label   Content="No. Socio" />
                        <andi:Text_Upload x:Name="txt_subir1"/>
                    </StackPanel>
                </Grid>
                <Grid Grid.Column="2">
                    <StackPanel Style="{StaticResource stack_margin-controls}">
                        <Label   Content="No. Socio" />
                        <andi:List_Check></andi:List_Check>
                    </StackPanel>
                </Grid>

            </Grid>

            <Grid Grid.Row="1">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="*"></ColumnDefinition>
                    <ColumnDefinition Width="*"></ColumnDefinition>
                    <ColumnDefinition Width="*"></ColumnDefinition>
                </Grid.ColumnDefinitions>
                <Grid Grid.Column="0">
                    <StackPanel Style="{StaticResource stack_margin-controls}" >
                        <Label   Content="No. Socio" />
                        <TextBox Style="{StaticResource control_heigh}" />
                    </StackPanel>
                </Grid>
                <Grid Grid.Column="1">
                    <StackPanel Style="{StaticResource stack_margin-controls}">
                        <Label   Content="No. Socio" />
                        <andi:Text_Upload  x:Name="txt_subir3"/>
                    </StackPanel>
                </Grid>
                <Grid Grid.Column="2">
                    <StackPanel Style="{StaticResource stack_margin-controls}">
                        <Label   Content="No. Socio" />
                        <andi:Text_Upload  x:Name="txt_subir4"/>
                    </StackPanel>
                </Grid>

            </Grid>

        </Grid>
    </Border>
</UserControl>
